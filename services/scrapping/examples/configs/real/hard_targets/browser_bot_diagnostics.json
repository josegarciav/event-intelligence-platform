    {
      "source_id": "bot_browserscan",
      "engine": {
        "type": "browser",
        "timeout_s": 30,
        "browser": {
          "headless": true,
          "save_artifacts": true
        },
        "rate_limit_policy": {
          "rps": 0.1,
          "min_delay_s": 5.0
        }
      },
      "entrypoints": [{ "url": "https://www.browserscan.net/bot-detection" }],
      "metadata": { "purpose": "diagnostic", "compliance": "detection_only" }
    }
  ]
}
